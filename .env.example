# Example environment variables for deployment
# Copy this file to .env.local or .env and fill values before deploying

# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key  # KEEP SECRET
POSTGRES_URL_NON_POOLING=postgres://postgres:password@db.your-host.supabase.co:5432/postgres?sslmode=require
SUPABASE_JWT_SECRET=your_supabase_jwt_secret

# Stripe
STRIPE_TEST_SECRET_KEY=sk_test_xxx
STRIPE_TEST_PUBLISHABLE_KEY=pk_test_xxx
STRIPE_LIVE_SECRET_KEY=sk_live_xxx
STRIPE_LIVE_PUBLISHABLE_KEY=pk_live_xxx
STRIPE_LIVE_MODE=false

# AI providers (if used)
NOVITA_API_KEY=your_novita_key
NEXT_PUBLIC_NOVITA_API_KEY=your_novita_key

# App
NEXTAUTH_URL=https://your-domain

